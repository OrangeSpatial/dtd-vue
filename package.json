{
  "name": "dtd-vue",
  "packageManager": "yarn@4.1.1",
  "version": "0.3.0",
  "type": "module",
  "main": "dist/es/index.mjs",
  "module": "dist/es/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./dist/es/index.mjs": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.build.ts"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OrangeSpatial/dtd-vue"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "9.x",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.3.0",
    "sass": "^1.75.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.0",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "vue": "^3.4.25"
  }
}
